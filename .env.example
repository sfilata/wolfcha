## LLM gateway (ZenMux / OpenRouter-compatible)
# Preferred (new)
ZENMUX_API_KEY=your_zenmux_api_key_here
# Optional: override chat completions endpoint (OpenRouter-compatible)
ZENMUX_CHAT_COMPLETIONS_URL=
#
# Backward-compatible (old)
OPENROUTER_API_KEY=
OPENROUTER_CHAT_COMPLETIONS_URL=

## TTS (MiniMax)
MINIMAX_API_KEY=your_minimax_api_key_here
MINIMAX_GROUP_ID=your_minimax_group_id_here
# Optional: https://api.minimaxi.com or https://api.minimax.chat
MINIMAX_API_BASE_URL=
# Optional
MINIMAX_TTS_MODEL=speech-01-turbo

## Supabase
NEXT_PUBLIC_SUPABASE_URL=
# Use either anon key or publishable key
NEXT_PUBLIC_SUPABASE_ANON_KEY=
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=
SUPABASE_SERVICE_ROLE_KEY=

## App (optional)
NEXT_PUBLIC_APP_VERSION=0.0.0
NEXT_PUBLIC_SHOW_DEVTOOLS=true
